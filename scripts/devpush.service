[Unit]
Description=/dev/push stack
After=docker.service network.target
Wants=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/opt/devpush
ExecStart=/opt/devpush/scripts/start.sh
ExecStop=/opt/devpush/scripts/stop.sh --down
ExecReload=/opt/devpush/scripts/restart.sh --no-migrate
TimeoutStartSec=600
TimeoutStopSec=600

[Install]
WantedBy=multi-user.target
